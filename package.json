{
  "name": "redux-saga-selector-channel",
  "version": "1.0.0",
  "description": "Create a redux-saga channel which updates with changes to a selector function.",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "prebuild": "npm run test",
    "build": "tsc --outDir dist",
    "clean": "rimraf dist",
    "lint": "eslint --ext ts src/**/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "posttest": "npm run lint",
    "prepack": "npm run build",
    "release": "np"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knpwrs/redux-saga-selector-channel.git"
  },
  "keywords": [
    "redux",
    "saga",
    "selector",
    "channel"
  ],
  "author": "Kenneth Powers <ken@kenpowers.net> (https://knpw.rs)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/knpwrs/redux-saga-selector-channel/issues"
  },
  "homepage": "https://github.com/knpwrs/redux-saga-selector-channel#readme",
  "dependencies": {
    "redux-saga": "^1.0.1"
  },
  "peerDependencies": {
    "redux": "^4.0.1",
    "redux-saga": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.3",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^2.6.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "husky": "^1.2.0",
    "jest": "^24.5.0",
    "np": "^4.0.2",
    "redux": "^4.0.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.2.1"
  }
}
